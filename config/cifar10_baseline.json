{
    "dataset": "cifar10",
    "pretrained": null,
    "batch_size": 128,
    "num_epochs": 200,
    "feature_dim": 32,
    "lr": 0.01,
    "lr_scheduler": {
        "trigger_epochs": [100, 180],
        "decay_factor": 0.1
    },
    "wd": 0.01,
    "training_type": "stochastic",
    "var_type": "full_rank",
    "reg_type": "wSw",
    "reg_weight": 1.0,
    "var_threshold": 16.0,
    "attack": "pgd",
    "epsilon": null,
    "adv_loss_weight": 0.5,
    "device": "cuda:0",
    "output_path": {
        "stats": "output/stats/sesnn_cifar10_b3_lr_0.01_regw_1.0",
        "models": "output/models/sesnn_cifar10_b3_lr_0.01_regw_1.0"
    },
    "monte_carlo_runs": 10
}
